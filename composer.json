{
    "name": "intervention/httpauth",
    "description": "HTTP Authentication Management for PHP",
    "homepage": "https://httpauth.intervention.io",
    "keywords": [
        "authentication",
        "http"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Oliver Vogel",
            "email": "oliver@intervention.io",
            "homepage": "https://intervention.io/"
        }
    ],
    "require": {
        "php": "^8.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0 || ^11.0",
        "phpstan/phpstan": "^2.1",
        "squizlabs/php_codesniffer": "^3.8",
        "slevomat/coding-standard": "~8.0"
    },
    "autoload": {
        "psr-4": {
            "Intervention\\HttpAuth\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Intervention\\HttpAuth\\Tests\\": "tests"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
